<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.woolution.shop.mapper.SampleMapper">

    <insert id="sampleRegister">
        insert into sample (comment, price)
        values (#{comment}, #{price})
    </insert>

    <select id="sampleList" resultType="hashmap">
        select id, comment, price from sample
    </select>

</mapper>